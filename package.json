    "ember-cli-htmlbars": "^6.3.0",
    "ember-concurrency": "^2.3.7",
    "ember-get-config": "^2.1.1",
    "ember-infinity": "^2.1.1",
    "ember-power-select": "^4.0.0"
  },
  "devDependencies": {
    "@ember/jquery": "^1.1.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.9.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.7.2",
    "ember-cli": "~3.28.6",
    "ember-cli-addon-docs": "^2.0.0",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-mirage": "^1.1.8",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-data": "~3.28.13",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-mirage-sauce": "^0.1.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.2",
    "ember-sinon": "^4.1.1",
    "ember-sinon-qunit": "^4.0.1",
    "ember-source": "~3.28.12",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.11.0",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "faker": "^5.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.20.0",
    "qunit-dom": "^1.5.0",
    "sass": "^1.17.0",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "homepage": "https://nickschot.github.io/ember-model-select",
