{
  "name": "ember-model-select",
  "version": "1.0.0-beta.1",
  "description": "An ember-cli addon to provide a searchable model select box with infinite scroll support.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nickschot/ember-model-select"
  },
  "license": "MIT",
  "author": "Nick Schot <nickschot@webhub.nl>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "npx lerna-changelog --from=v0.1.0-alpha.7 > CHANGELOG.md",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "node ./compile-css.js"
  },
  "dependencies": {
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "ember-cli-babel": "^7.18.0",
    "ember-cli-htmlbars": "^4.2.3",
    "ember-concurrency": "^1.1.0",
    "ember-concurrency-test-waiter": "^0.3.1",
    "ember-get-config": "^0.2.4",
    "ember-infinity": "^2.1.1",
    "ember-power-select": "^4.0.0"
  },
  "devDependencies": {
    "@ember/jquery": "^0.6.0",
    "@ember/optional-features": "^1.3.0",
    "babel-eslint": "^8.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.17.0",
    "ember-cli-addon-docs": "^0.7.0",
    "ember-cli-addon-docs-yuidoc": "^0.2.4",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-mirage": "^0.4.10",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-concurrency-decorators": "^1.0.0",
    "ember-data": "~3.12.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-mirage-sauce": "git+https://github.com/nickschot/ember-mirage-sauce.git#fixes",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-sinon": "^4.1.1",
    "ember-sinon-qunit": "^4.0.1",
    "ember-source": "~3.17.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.4.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^7.10.1",
    "eslint-plugin-node": "^11.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.1.0",
    "sass": "^1.17.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "homepage": "https://nickschot.github.io/ember-model-select"
}
